{
  "name": "knockknock",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator=iPhone 14 Pro",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "build-staging-android": "react-native run-android --mode=releaseStaging",
