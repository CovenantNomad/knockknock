    "code-push-staging-android": "appcenter codepush release-react -a Covenant-Nomad/KnockKnock-Android -d Staging",
    "code-push-prod-ios": "appcenter codepush release-react -a Covenant-Nomad/KnockKnock-iOS -d Production",
    "code-push-prod-android": "appcenter codepush release-react -a Covenant-Nomad/KnockKnock-Android -d Production"
  },
